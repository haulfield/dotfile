.com';\n$cc = 'cc@example.com';\n$from = 'from@example.com';\n$subject = 'Hello world';\n$message = 'This is test email sent by Perl Script';\n\n$msg = MIME::Lite->new(\n  From     => $from,\n  To       => $to,\n  Cc       => $cc,\n  Subject  => $subject,\n  Type     => 'multipart/mixed'\n);\n\n# Add your text message.\n$msg->attach(Type => 'text', Data => $message);\n\n# Specify your file as attachement.\n$msg->attach(Type        => 'image/gif',\n             Path        => '/tmp/logo.gif',\n             Filename    => 'logo.gif',\n             Disposition => 'attachment'\n);\n# config\n$msg->send('smtp', \""+e.escapeExpression((a=null!=(a=n.domain||(null!=t?t.domain:t))?a:n.helperMissing,"function"==typeof a?a.call(null!=t?t:{},{name:"domain",hash:{},data:r}):a))+'", Port=>'+e.escapeExpression((a=null!=(a=n.smtp_main_port||(null!=t?t.smtp_main_port:t))?a:n.helperMissing,"function"==typeof a?a.call(null!=t?t:{},{name:"smtp_main_port",hash:{},data:r}):a))+', Hello=>"'+e.escapeExpression((a=null!=(a=n.domain||(null!=t?t.domain:t))?a:n.helperMissing,"function"==typeof a?a.call(null!=t?t:{},{name:"domain",hash:{},data:r}):a))+'", AuthUser=>"'+e.escapeExpression((a=null!=(a=n.username||(null!=t?t.username:t))?a:n.helperMissing,"function"==typeof a?a.call(null!=t?t:{},{name:"username",hash:{},data:r}):a))+'", AuthPass=>"'+e.escapeExpression((a=null!=(a=n.password||(null!=t?t.password:t))?a:n.helperMissing,"function"==typeof a?a.call(null!=t?t:{},{name:"password",hash:{},data:r}):a))+'" );\nprint "Email Sent Successfully\\n";</code></pre>\n  </div>\n\n  <div class="select-content hidden tab_perl_net_smtp">\n    <pre><code class="bash">#!/usr/bin/perl\nuse strict;\n\nuse Cwd;\nuse Data::Dumper;\nuse Net::SMTP;\nuse File::Slurp;\n\nsub sendMail($$);\n\n\nuse constant DEBUG => 0;\n\nmy $startDir = getcwd;\n\nmy %email = ( \'smtp\' => \''+e.escapeExpression((a=null!=(a=n.domain||(null!=t?t.domain:t))?a:n.helperMissing,"function"==typeof a?a.call(null!=t?t:{},{name:"domain",ha