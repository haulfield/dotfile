{
   "background": {
      "scripts": [ "js/screen-share-background.js", "lib/jquery-2.1.1.min.js", "lib/lsapi.min.js", "js/api.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon128.png",
      "default_popup": "html/popup.html",
      "default_title": ""
   },
   "content_scripts": [ {
      "css": [ "lib/chosen/chosen.min.css", "css/gcal.css" ],
      "js": [ "lib/jquery-2.1.1.min.js", "lib/chosen/chosen.jquery.min.js", "js/HtmlHelpers.js", "js/content-script.js" ],
      "matches": [ "https://www.google.com/calendar/*", "https://calendar.google.com/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "js/screen-share-content.js" ],
      "matches": [ "https://*.lifesizecloudbeta.com/*", "https://*.lifesizeclouddev.com/*", "https://*.lifesizecloud.com/*" ]
   } ],
   "default_locale": "en",
   "description": "Schedule Lifesize Cloud Meetings from Google Calendar & Share Content in Lifesize Cloud Web App Video Calls from Google Chrome",
   "icons": {
      "128": "img/icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Lifesize",
   "options_page": "html/options.html",
   "permissions": [ "tabs", "storage", "https://*.lifesizecloudbeta.com/*", "https://*.lifesizeclouddev.com/*", "https://*.lifesizecloud.com/*", "https://www.google.com/calendar/*", "https://calendar.google.com/*", "activeTab", "desktopCapture" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.7.1",
   "web_accessible_resources": [ "img/*", "html/*", "css/*", "lib/*", "js/sidebar.js" ]
}
